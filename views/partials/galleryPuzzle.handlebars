<div class = "puzzle-container">
    <!-- Turn this into Cool Puzzle Template -->
    <article class = "puzzle">
        <div class = "img-container">
            <img class = "puzzle-photo" src = {{path}}>
        </div>
        <span class = "like like-icon">
            <i class="fas fa-heart"></i>
        </span>
        <span class = "like like-number">
            {{likes}}
        </span>
    </article>
</div>